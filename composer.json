{
    "name": "cdaguerre/php-textmaster-api",
    "type": "library",
    "description": "Textmaster API v3 client",
    "homepage": "https://github.com/cdaguerre/php-textmaster-api",
    "keywords": ["textmaster", "translation", "api"],
    "license": "UNLICENSED",
    "authors": [
        {
            "name": "Christian Daguerre",
            "email": "christian@daguer.re"
        }
    ],
    "require": {
        "php":             ">=5.3.2",
        "ext-curl":        "*",
        "guzzle/guzzle":   "~3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fabpot/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": { "Textmaster\\": "lib/Textmaster/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
